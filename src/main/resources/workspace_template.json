{
  "environments": {
    "default": {
      "machines": {
        "dev-machine": {
          "servers": {},
          "agents": [
            "org.eclipse.che.terminal",
            "org.eclipse.che.ws-agent",
            "org.eclipse.che.ssh"
          ],
          "attributes": {
            "memoryLimitBytes": "2147483648"
          }
        }
      },
      "recipe": {
        "location": "eclipse/ubuntu_jdk8",
        "type": "dockerimage"
      }
    }
  },
  "defaultEnv": "default",
  "name": "${workspace.name}",
  "description": null,
  "commands": [
    {
      "commandLine": "mvn clean install -f ${current.project.path}",
      "name": "build",
      "type": "mvn",
      "attributes": {}
    }
  ],
  "projects": [
    {
      "name": "console-java-simple",
      "type": "maven",
      "description": "A hello world Java application.",
      "path": "/console-java-simple",
      "attributes": {},      
      "source": {
        "location": "${workspace.git.repo}",
        "type": "git",
        "parameters": {
          "branch": "${workspace.git.branch}",
          "keepVcs": "true",
        }
      },
      "mixins": [ "git" ],
    }
  ],
  "links": []
}
